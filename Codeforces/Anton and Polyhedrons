    #include<iostream>
    #include<vector>
    #include<stack>
    #include<algorithm>
    #include<math.h>
    #include<queue>
    #define pb(x) push_back(x)
    #define p(x)  push(x)
     
     
    using namespace std;
     
     
    int main(){
     
    				
   	int n;
    				
    				
   cin>>n;
    				
   string a;
    				
    		
    				
   int c=0;
    				
   for(int i=0;i<n;i++){
    					
   	cin>>a;
    					
    		
    					
   	if(a=="Tetrahedron")c=c+4;
   if(a=="Cube")c=c+6;
   if(a=="Octahedron")c=c+8;
   if(a=="Dodecahedron")c=c+12;
   	if(a=="Icosahedron")c=c+20;
    					
  }
    			
       cout<<c<<endl;
                     
   }
