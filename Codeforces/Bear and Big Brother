    #include<iostream>
    #include<vector>
    #include<stack>
    #include<algorithm>
    #include<math.h>
    #include<queue>
    #define pb(x) push_back(x)
    #define p(x)  push(x)
     
     
    using namespace std;
     	 
     
     
    int main(){
                    	
   	int a,b;
                    	
   	cin>>a>>b;
                    	
   	int c=0;
                    	
   	while(a<b){
    						
    						
    a=a*3; b=b*2;
    						
    if(a>b)break;
    						
    c++;
    	}
     
   cout<<c+1<<endl;
    						
 }
