#include<iostream>
#include<algorithm>
#include<math.h>
 
		using namespace std;
				
  	int main(){
 
	  string a,b;int n=0,m=0;
		cin>>a>>b;
			
			
		if(a==b)
		cout<<-1<<endl;
		else{
			
		for(int i=0;a[i]!='\0';i++)
				m++;
		for(int i=0;b[i]!='\0';i++)
			n++;
		cout<<max(m,n)<<endl;
				
			}
		}
