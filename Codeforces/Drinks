    #include<bits/stdc++.h>
     
    using namespace std;
    						
    int main(){
    			   			
  		int   n;    			
    	cin>>n;
    			
     int ar[n+1];
    			
    for(int i=0;i<n;i++)				
    		cin>>ar[i];
    				
    	double sum=0;	   			
    			
  	for(int i=0;i<n;i++)
    	 sum=sum+ar[i];
    			
    			
   	double  r=n*100;  			
   	double rslt;  rslt=sum/r; 
    double ans; ans=rslt*100; 
    cout<<fixed<<setprecision(7)<<ans<<endl;
 	}
