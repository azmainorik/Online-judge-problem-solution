    #include<iostream>
     
     
    using namespace std;
    
     
    int main(){
     
     
   long long  int l1,r1,l2,r2,a,k;
         
   cin>>l1>>r1>>l2>>r2>>k;
     
     
    if(r2<l1||l2>r1)
      cout<<0<<endl;
    else{
      if(l1<=l2)
        l1=l2;
   
   if(r1>=r2)
     r1=r2;
     
     
   a=r1-l1+1;
     
   if(k<=r1&&k>=l1)
     a=a-1;
     
  cout<<a<<endl;
     
  }
     
 }
