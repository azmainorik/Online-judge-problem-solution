    #include<iostream>
         
     
     using namespace std;
     
     
      int main(){
     
      long long int n;
     
      cin>>n;
     
      long long  int arr[n+1];
     
     
      for(int i=0;i<n;i++)
          cin>>arr[i];
     
      long long int sum=0;
     
      for(int j=0;j<n-1;j++)
      {
         sum=arr[j]+arr[j+1];
            cout<<sum<<" ";
       }
     
      cout<<arr[n-1]<<endl;
              
   }
