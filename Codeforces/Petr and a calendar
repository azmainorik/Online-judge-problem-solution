#include<iostream>
 
 
using namespace std;
			
int main(){
			
int m,d;
			
cin>>m>>d;
			
if(m!=2){	

if(m==1||m==3||m==5||m==7||m==8||m==10||m==12)
{		
				if(d>=1&&d<6)						
				cout<<5<<endl;
				else
				cout<<6<<endl;	
}
else
{
	if(d>=1&&d<=6)						
	cout<<5<<endl;
	else
	cout<<6<<endl;	
}
}
else
{
	if(d>1&&d<=7)						
	cout<<5<<endl;
	else
	cout<<4<<endl;	
 
}
			
}
