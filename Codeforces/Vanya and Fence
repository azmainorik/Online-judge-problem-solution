#include<iostream>
#include<math.h>
#include<algorithm>
#include<vector>
#define pb(x) push_back(x) 
 
 
 
			using namespace std;
			
			int main(){
				
				int n,h;
				
				
				cin>>n>>h;
				
				
				int arr[n+1];int c=0;
				
				
				for(int i=0;i<n;i++){
						
						
					cin>>arr[i];
							
						if(arr[i]>h)c++;	
							
				}
				
				cout<<(n-c)*1+(c*2)<<endl;
				
				
			}
				
